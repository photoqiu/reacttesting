{
    "name": "reactsample",
    "version": "1.0.0",
    "description": "react.js demo datas",
    "main": "'",
    "scripts": {
        "dev": "webpack serve --mode development --env development --config ./webpack.dev.config.js",
        "testing": "webpack --config ./webpack.testing.config.js",
        "build": "webpack --mode production cross-env NODE_ENV=production webpack --config ./webpack.prod.config.js",
        "lint": "eslint ./ --cache --fix --ignore-pattern .gitignore",
        "dll": "webpack --config ./webpack.dll.config.js",
        "test": "node --trace-deprecation node_modules/webpack/bin/webpack.js  --config ./webpack.dev.config.js"
    },
    "repository": {
        "type": "git",
        "url": "9.91"
    },
    "keywords": [
        "reactsample"
    ],
    "author": "photoqiu",
    "license": "MIT",
    "jest": {
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "testEnvironment": "jsdom",
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy",
            "^@apis(.*)$": "<rootDir>/app/apis$1",
            "^@configs(.*)$": "<rootDir>/app/configs$1",
            "^@ajax(.*)$": "<rootDir>/app/configs/ajax.js",
            "^@config(.*)$": "<rootDir>/app/configs/config.js",
            "^@reg(.*)$": "<rootDir>/app/configs/regular.config.js",
            "^@components(.*)$": "<rootDir>/app/components$1",
            "^@tableList(.*)$": "<rootDir>/app/components/tableList/tableList.js",
            "^@redux/actions(.*)$": "<rootDir>/app/redux/actions$1",
            "^@images(.*)$": "<rootDir>/app/images$1"
        },
        "transform": {
            "^.+\\.js$": "babel-jest"
        },
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/coverage/**",
            "!**/scripts/**",
            "!**/reducers/**",
            "!**/actions/**",
            "!**/base/**",
            "!**/apis/**",
            "!**/mocks/**",
            "!**/middleware/**",
            "!**/components/**",
            "!**/menu/**",
            "!**/configs/**",
            "!**/store.js",
            "!**/provider.jsx",
            "!**/example.js",
            "!**/client.js",
            "!**/webpack.config.js",
            "!**/selectRole.js",
            "!**/setCenter/index.js",
            "!**/dist/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 50,
                "functions": 50,
                "lines": 50,
                "statements": 50
            }
        }
    },
